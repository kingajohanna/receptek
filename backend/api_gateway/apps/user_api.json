{
    "name": "User API",
    "api_id": "a0764d84-d854-4d0e-8342-6b0b393f56e0",
    "definition": {
        "location": "header",
        "key": "version"
    },
    "auth": {
        "auth_header_name": "authorization"
    },
    "enable_context_vars": true,
    "version_data": {
        "not_versioned": true,
        "versions": {
          "Default": {
            "name": "Default",
            "paths": {
              "ignored": [],
              "white_list": [],
              "black_list": []
            },
            "use_extended_paths": true,
            "extended_paths": {
              "ignored": [],
              "white_list": [],
              "black_list": [],
              "cache": ["get"],
              "transform": [
                {
                  "path": "/add/",
                  "method": "POST",
                  "template_data": {
                    "template_mode": "file",
                    "template_source":"/opt/tyk-gateway/custom-templates/add_user.tmpl",
                    "input_type": "json",
                    "enable_session": false
                  }
                }
              ],
              "transform_headers": [],
              "url_rewrites": [
                {
                "path": "/del/",
                "method": "DELETE",
                "match_pattern": "/del/",
                "rewrite_to": "/user/del/$tyk_context.jwt_claims_user_id/"
              }]
            },
            "global_headers":{
              "X-user": "$tyk_context.jwt_claims_user_id"
            },
            "global_headers_remove": ["authorization"]
          }
        }
      },
    "use_openid": true,
    "openid_options": {
    "providers": [
    {
      "issuer": "https://securetoken.google.com/mealo-mao",
      "client_ids": {
        "bWVhbG8tbWFv": "default"
      }
    }
    ]},
    "segregate_by_client": false,
    "proxy": {
        "listen_path": "/user/",
        "target_url": "http://192.168.1.168:8001/",
        "strip_listen_path": false,
        "preserve_host_header": true
    },
    "active": true
}